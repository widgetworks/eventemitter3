{
  "name": "@wiwo/eventemitter3",
  "version": "6.0.0",
  "description": "EventEmitter3 focuses on performance while maintaining a Node.js AND browser compatible interface.",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --sourcemap",
    "test": "jest --env=node",
    "test-browser": "jest --env=jsdom"
  },
  "disabledScripts": {
    "prepublishOnly": "npm run build",
    "minify": "uglifyjs umd/eventemitter3.js --source-map -cm -o umd/eventemitter3.min.js",
    "benchmark": "find benchmarks/run -name '*.js' -exec benchmarks/start.sh {} \\;"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/widgetworks/eventemitter3.git"
  },
  "keywords": [
    "EventEmitter",
    "EventEmitter2",
    "EventEmitter3",
    "Events",
    "addEventListener",
    "addListener",
    "emit",
    "emits",
    "emitter",
    "event",
    "once",
    "pub/sub",
    "publish",
    "reactor",
    "subscribe"
  ],
  "author": "Arnout Kazemier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/widgetworks/eventemitter3/issues"
  },
  "devDependencies": {
    "assume": "^2.2.0",
    "babel-jest": "^29.0.3",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "pre-commit": "^1.2.0",
    "ts-jest": "^29.0.2",
    "tsup": "^6.2.3",
    "uglify-js": "^3.9.0"
  }
}
